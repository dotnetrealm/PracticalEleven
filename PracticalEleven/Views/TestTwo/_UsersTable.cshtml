@model IEnumerable<User>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Date of birth</th>
            <th scope="col">Address</th>
            <th scope="col">Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <th scope="row">@Html.DisplayFor(data => user.Id)</th>
                <td>@Html.DisplayFor(data => user.Name)</td>
                <td>@Html.DisplayFor(data => user.DOB)</td>
                <td>@Html.DisplayFor(data => user.Address)</td>
                <td>
                    <a class="text-dark me-2" asp-controller="TestTwo" asp-action="UserDetails" asp-route-id="@user.Id"><i class="fa fa-eye"></i></a>
                    <a class="text-dark me-2" asp-controller="TestTwo" asp-action="Edit" asp-route-id="@user.Id"><i class="fa fa-user-pen"></i></a>
                    <a class="text-dark" onclick="return confirm('Are you sure you want to delete this?')" asp-controller="TestTwo" asp-action="Delete" asp-route-id="@user.Id"><i class="fa fa-trash"></i></a>
                </td>
            </tr>
        }
    </tbody>
</table>