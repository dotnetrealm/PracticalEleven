@model User

<div class=" py-4 my-3">
    <div class="d-flex gx-4">
        <h5 class="mx-3"><i class="fa fa-solid fa-user me-2"></i>@Html.DisplayFor(user => Model.Name)</h5>
        <h5 class="mx-3"><i class="fa fa-solid fa-calendar me-2"></i>@Html.DisplayFor(user => Model.DOB)</h5>
        <h5 class="mx-3"><i class="fa fa-solid fa-map-marker me-2"></i> @Html.DisplayFor(user => Model.Address)</h5>
    </div>
</div>
